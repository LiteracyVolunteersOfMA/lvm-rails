<%= simple_form_for(@tutoring_session) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :location %>
      <%= f.input :hours %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.input :session_comment %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>
