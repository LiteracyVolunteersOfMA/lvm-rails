<div class="row">
  <div class="col-md-12">
    <h1 class="text-center">
      Edit Coordinator<br>
      <small> <%= link_to @coordinator.name, @coordinator%> </small>
    </h1>
  </div>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <hr>
    <%= simple_form_for :coordinator, url: coordinator_path(@coordinator), method: :patch do |f| %>
      <%= render partial: 'form', locals: {f: f} %>
    <% end %>

  </div>
</div>
